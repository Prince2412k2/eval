# LlamaIndex/LlamaParse API Key
LLAMAINDEX=your_llama_cloud_api_key_here

# PostgreSQL Database URL (external/hosted)
DB_URL=postgresql://user:password@host:5432/database_name

# Groq API Key (for LLM inference)
GROQ_API=your_groq_api_key_here

# Qdrant Vector Database (external/hosted)
QDRANT_URL=https://your-qdrant-instance.cloud:6333
QDRANT_API_KEY=your_qdrant_api_key_here

# Supabase (for file storage)
SUPABASE_URL=https://your-project.supabase.co
SUPABASE_KEY=your_supabase_anon_key_here

# Application Secret Key (generate with: openssl rand -hex 32)
SECRET_KEY=your_random_secret_key_here
